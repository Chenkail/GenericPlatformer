        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw buttons
//Set variables
buttonX = rowLeft + buttonWidth/2;

//Draw each button
for (var index = 0; index &lt; 4; index++) {
    //Draw button background
    buttonLeft = buttonX - buttonWidth/2;
    buttonRight = buttonLeft + buttonWidth;
    buttonCornerRadius = 10;
    buttonFillColor = c_white;
    buttonOutlineColor = c_black;
    //Fill
    draw_roundrect_colour_ext(buttonLeft, rowTop, buttonRight, rowBottom,
                                buttonCornerRadius, buttonCornerRadius,
                                buttonFillColor, buttonFillColor, false);
    //Outline
    draw_roundrect_colour_ext(buttonLeft, rowTop, buttonRight, rowBottom,
                                buttonCornerRadius, buttonCornerRadius,
                                buttonOutlineColor, buttonOutlineColor, true);
    //Draw sprite
    sprite = scr_level_select_page_button_sprite(index);
    draw_sprite(sprite, 0, buttonX, rowY);
    
    //Move to next button
    buttonX = buttonX + buttonWidth + spacing;
}

</string>
          </argument>
        </arguments>
