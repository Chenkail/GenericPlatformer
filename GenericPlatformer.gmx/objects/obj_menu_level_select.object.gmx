        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Set up level select menu boxes
//Local variable assignments
columns = global.levelSelectColumns;
rows = global.levelSelectRows;
numberOfLevels = global.numberOfLevels;
levelsPerPage = columns * rows;
global.levelsPerPage = columns * rows;
numberOfPages = int64(numberOfLevels / levelsPerPage)
if ((global.numberOfLevels mod global.levelsPerPage) != 0) {
    numberOfPages++;
}
global.levelSelectPageCount = numberOfPages;
choiceWidth = global.levelSelectChoiceWidth;
choiceHeight = global.levelSelectChoiceHeight;
choiceColor = global.levelSelectChoiceColor;
choiceOutline = global.levelSelectChoiceOutline;
spacing = global.levelSelectSpacing;
buffer = global.levelSelectBoxBuffer;

//Calculations for large box
//Do not touch!
boxWidth = columns * choiceWidth //Total width of option boxes
    + (columns - 1) * spacing //Total width of spacing between boxes
    + 2 * buffer; //Buffer spacing on both sides
boxHeight = rows * choiceHeight //Total height of option boxes
    + (rows - 1) * spacing //Total height of spacing between boxes
    + 2 *  buffer; //Buffer spacing on top/bottom
cornerRadius = min(boxWidth, boxHeight) * global.levelSelectBoxCornerRadiusProportion; //Corner radius dependant on smaller dimension
choiceCornerRadius = min(choiceWidth, choiceHeight) * global.levelSelectChoiceCornerRadiusProportion; //Set corner radius to be 1/4 of the smaller dimension of the choice boxes
boxLeft = room_width/2 - boxWidth/2; //Left edge
boxTop = room_height/2 - boxHeight/2 + global.levelSelectBoxHeightOffset; //Top edge
boxRight = boxLeft + boxWidth; //Right edge
boxBottom = boxTop + boxHeight; //Bottom edge
global.levelSelectBoxBottom = boxBottom;

</string>
          </argument>
        </arguments>
