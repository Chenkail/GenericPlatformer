            <string>///Check if player in door frame and if so, enter door when down arrow is pressed.
//Define coordinates to check
doorX1 = x - global.doorWidth/2;
doorY1 = y - global.doorHeight/2;
doorX2 = doorX1 + global.doorWidth;
doorY2 = doorY1 + global.doorHeight;

//Check to see if player is in door boundary
playerInDoor = false;
playerInDoor = collision_rectangle(doorX1, doorY1, doorX2, doorY2, obj_player, false, true);

//Check to see if player has all the fish
allFishCollected = false;
if (global.levelCollectedFish = global.levelTotalFish) {
    allFishCollected = true;
}


if ((playerInDoor) and (!currentlyFading)) {
    if (keyboard_check_pressed(vk_down) or keyboard_check_pressed(ord('S'))) {
        if (allFishCollected) {
            //Mark level as complete in ini file
            ini_open("savedata.ini");
            ini_write_real("Levels", global.currentLevel, 1);
            ini_close();
            
            if (room != rm_game_complete) {
                //Go to next level if not on game complete screen
                nextRoom = room_next(room);
            } else {
                //If game complete return to level select
                nextRoom = rm_level_select;
            }
            scr_fade_to_room(nextRoom, global.doorFadeColor);
            currentlyFading = true;
        } else {
            //Draw need more fish
            scr_need_more_fish(x, y);
        }
    }
}
</string>
