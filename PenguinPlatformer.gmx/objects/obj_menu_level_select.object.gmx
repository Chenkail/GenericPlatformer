            <string>///Set up level select menu
//Calculations for global variables
global.levelsPerPage = global.levelSelectColumns * global.levelSelectRows;
global.levelSelectPageCount = int64(global.numberOfLevels / global.levelsPerPage)
if ((global.numberOfLevels mod global.levelsPerPage) != 0) {
    global.levelSelectPageCount++;
}

//Calculations for large box
//Do not touch!
global.levelSelectBoxWidth = global.levelSelectColumns * global.levelSelectChoiceWidth //Total width of option boxes
    + (global.levelSelectColumns - 1) * global.levelSelectSpacing //Total width of spacing between boxes
    + 2 *  global.levelSelectBoxBuffer; //Buffer spacing on both sides
global.levelSelectBoxHeight = global.levelSelectRows * global.levelSelectChoiceHeight //Total height of option boxes
    + (global.levelSelectRows - 1) * global.levelSelectSpacing //Total height of spacing between boxes
    + 2 *  global.levelSelectBoxBuffer; //Buffer spacing on top/bottom
global.levelSelectBoxCornerRadius = min(global.levelSelectBoxWidth, global.levelSelectBoxHeight) * global.levelSelectBoxCornerRadiusProportion; //Corner radius dependant on smaller dimension
global.levelSelectChoiceCornerRadius = min(global.levelSelectChoiceWidth, global.levelSelectChoiceHeight) * global.levelSelectChoiceCornerRadiusProportion; //Set corner radius to be 1/4 of the smaller dimension of the choice boxes
global.levelSelectBoxLeft = room_width/2 - global.levelSelectBoxWidth/2; //Left edge
global.levelSelectBoxTop = room_height/2 - global.levelSelectBoxHeight/2 + global.levelSelectBoxHeightOffset; //Top edge
global.levelSelectBoxRight = global.levelSelectBoxLeft + global.levelSelectBoxWidth; //Right edge
global.levelSelectBoxBottom = global.levelSelectBoxTop + global.levelSelectBoxHeight; //Bottom edge

//Page control buttons
global.levelSelectPageControlButtonCount = 4;
global.pageButtonRowWidth = (global.levelSelectPageControlButtonCount - 1) * (global.levelSelectPageButtonWidth + global.levelSelectPageButtonSpacing);
global.levelSelectPageButtonY = global.levelSelectBoxBottom + global.levelSelectPageButtonYSpacing + global.levelSelectPageButtonHeight/2;
global.levelSelectPageButtonCornerRadius = min(global.levelSelectPageButtonWidth, global.levelSelectPageButtonHeight) * global.levelSelectPageButtonCornerRadiusProportion;
</string>
