        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement
//Set player speed
global.playerSpeed = global.playerBaseSpeed * global.blockSpeedModifier;

//Set player image speed
global.playerImageSpeedWalk = global.playerBaseImageSpeedWalk * global.blockImageSpeedModifier;

//Define player side coordinates
playerLeft = x - playerWidth/2;
playerRight = playerLeft + playerWidth;
playerTop = y - playerHeight/2;
playerBottom = playerTop + playerHeight;

if (!global.playerTraveling) {
    switch (keyboard_key) {
        //Go left
        case vk_left:
        case ord("A"):
            canGoLeft = scr_can_move(playerLeft, y, true, playerHeight);
            if (canGoLeft) {
                sprite_index = spr_player_left;
                scr_walk(-1)
            }
            break;
        //Go right
        case vk_right:
        case ord("D"):
            canGoRight = scr_can_move(playerRight, y, true, playerHeight);
            if (canGoRight) {
                sprite_index = spr_player_right;
                scr_walk(1);
            }
            break;
        //Jump/Climb ladder
        case vk_up:
        case ord("W"):
            //Check if space above player is not a solid block
            canGoUp = scr_can_move(x, playerTop, false, playerWidth);
            movableBlockAbove = collision_rectangle(playerLeft + global.moveCheckForgiveness, playerTop - 1, playerRight - global.moveCheckForgiveness, playerTop, obj_movable_block, false, true);
            if (canGoUp) and (!movableBlockAbove) {
                ///Jump or climb ladder
                //If not on ladder, jump
                if (!global.playerOnLadder) {
                    scr_jump();
                } else {
                    //If on ladder, climb upwards
                    scr_climb_ladder(-1);
                }
            }
            break;
        case vk_down:
        case ord("S"):
            if (global.playerOnLadder) {
                //If on ladder, climb downwards
                scr_climb_ladder(1);
            }
            break;
    }
}

</string>
