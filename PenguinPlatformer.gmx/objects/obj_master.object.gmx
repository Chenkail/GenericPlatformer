global.playerSpeed = global.playerBaseSpeed;
global.blockSpeedModifier = 0;

///Blocks
//Speed constants for different block types
global.blockSpeedWood = 1; //Walking speed multiplier wood
global.blockSpeedIce = 1.6; //Walking speed multiplier ice

//Image speed constants for different block types
global.blockImageSpeedWood = global.blockSpeedWood;
global.blockImageSpeedIce = global.blockSpeedIce;

//Defining global variables for player image speed
//Do not touch!
global.blockImageSpeedModifier = global.blockImageSpeedWood;

///Jumping and falling
//Initalize jump permission to true
global.canJump = true; //Do not touch!

//Set jump constants
global.playerJumpTime = 0.4; //Length of player jump in seconds
global.jumpCheckConstant = 1; //Pixels below player to test for object
global.playerJumpHeightConstant = 10; //Height above player's head to which player can jump

//Defining global variables for player jump
//Do not touch!
global.playerDefaultJumpHeight = global.playerHeight + global.playerJumpHeightConstant;
global.playerJumpHeight = global.playerDefaultJumpHeight;


//Set gravity and max fall speed, along with fall speed constant
global.gravity = 6;
global.maxFallSpeed = 10; //Maximum player fall speed in pixels per step
global.fallSpeed = 5; //Initialize global fall speed variable and set it to a positive value so that the player falls if starting a level in the air

///Door Variables
global.doorFadeColor = c_black; //Color to fade to when transitioning through a doorway
//Door dimensions
//Do not touch unless door sprite has been changed
global.doorWidth = 64; //Width of door object in pixels
global.doorHeight = 64; //Height of door object in pixels

///Effect variables
//Fade to next room
global.fadeTime = 3; //Length of fade in seconds
global.fadePerTick = 0.05; //How dark should it get per tick (lower number means smoother fade)

//Defining global variables for fade effect
//Do not touch!
global.maxFadeAlpha = 1; //1.0 is a complete fade to the color
global.ticksToFade = global.maxFadeAlpha/global.fadePerTick;
global.stepsToFade = global.fadeTime * global.stepsPerSecond;
global.stepsPerTick = global.stepsToFade/global.ticksToFade;

