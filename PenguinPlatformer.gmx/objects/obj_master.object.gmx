global.gameTitleFont = fnt_title;
global.gameTitleFontColor = c_white;

//Button size
global.mainMenuButtonWidth = 100;
global.mainMenuButtonHeight = 50;

//Play button
global.playButtonX = room_width/2;
global.playButtonY = room_height/2;

///Level select menu
//Number of levels to display
global.levelSelectRows = 2;
global.levelSelectColumns = 4;

//Size constants
global.levelSelectChoiceWidth = 200;
global.levelSelectChoiceHeight = 200;
global.levelSelectSpacing = 25;
global.levelSelectBoxBuffer = 25;
global.levelSelectChoiceCornerRadius = min(global.levelSelectChoiceWidth, global.levelSelectChoiceHeight)/4;

//Title
global.levelSelectTitle = "Level Select";
global.levelSelectTitleY = 100;
global.levelSelectTitleFont = fnt_title;
global.levelSelectTitleFontColor = c_white;

//Box colors
global.levelSelectBoxColor = c_white;
global.levelSelectChoiceColor = c_black;
global.levelSelectChoiceOutline = true;
global.levelSelectChoiceFont = fnt_title;
global.levelSelectChoiceFontColor = c_black;
global.levelSelectChoiceHighlightColor = c_black;
global.levelSelectChoiceHighlightOutline = false;
global.levelSelectChoiceHighlightFont = fnt_title;
global.levelSelectChoiceHighlightFontColor = c_white;

//Constants calculation for drawing
//Do not touch!
global.levelSelectBoxHeightOffset = global.levelSelectTitleY;
global.levelSelectBoxWidth =
    global.levelSelectColumns * global.levelSelectChoiceWidth //Total width of option boxes
    + (global.levelSelectColumns - 1) * global.levelSelectSpacing //Total width of spacing between boxes
    + 2 *  global.levelSelectBoxBuffer; //Buffer spacing on both sides
global.levelSelectBoxHeight =
global.levelSelectRows * global.levelSelectChoiceHeight //Total height of option boxes
    + (global.levelSelectRows - 1) * global.levelSelectSpacing //Total height of spacing between boxes
    + 2 *  global.levelSelectBoxBuffer; //Buffer spacing on top/bottom
global.levelSelectBoxX1 = room_width/2 - global.levelSelectBoxWidth/2; //Left edge
global.levelSelectBoxY1 = room_height/2 - global.levelSelectBoxHeight/2 + global.levelSelectBoxHeightOffset; //Top edge
global.levelSelectBoxX2 = global.levelSelectBoxX1 + global.levelSelectBoxWidth; //Right edge
global.levelSelectBoxY2 = global.levelSelectBoxY1 + global.levelSelectBoxHeight; //Bottom edge
global.levelSelectBoxCornerRadius = min(global.levelSelectBoxWidth, global.levelSelectBoxHeight)/4; //Corner radius dependant on larger dimension

///Other Control Constants
//Define game steps per second
